{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "Storage": {
        "Limits": {
            "DatasetVersionPayload": {
                "MaxFileSize": 10000,
                "MaxFileCount": 3,
                "MaxTotalSize": 2000
            }
        },
        "ActiveStorageService": "FileSystemStorageService",
        "FileSystemStorageService": {
            "BasePath": "/var/data",
            "TempFilePath": "/var/tmp"
        },
        "S3StorageService": {
            "ServiceUrl": "http://localhost:9000",
            "BucketName": "test",
            "AccessKey": "test",
            "SecretKey": "testtest",
            "ForcePathStyle": true,
            "MultiPartUploadThreshold": 104857600,
            "MultiPartUploadChunkSize": 10485760
        },
        "NextCloudStorageService": {
            "BaseUrl": "http://localhost:8090",
            "BasePath": "test",
            "TempFilePath": "tmp",
            "User": "test",
            "Password": "test",
            "ChunkedUploadThreshold": 104857600,
            "ChunkedUploadChunkSize": 10485760
        }
    },
    "Authorization": {
        "JwksUri": "http://localhost:8080/jwks",
        "CorsAllowedOrigins": [
            "http://localhost:8080"
        ]
    },
    "PublicUrl": "http://localhost:8080"
}